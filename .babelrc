{
  "presets": [['es2015', { 'modules': false }], 'react', 'stage-0'],
  "env": {
    "test": {
      "presets": ['es2015', 'react', 'stage-0'],
    },
  },
  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "SPECS": "./spec",
        "APP_ROOT": "./app",
        "APP_ASSETS": "./assets",
        "MODULES": "./app/modules"
      }
    }]
  ]
}


